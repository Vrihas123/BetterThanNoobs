<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.in.out.hackathon.inoutapp.fragments.ConfirmBookingFragment">

    <TextView
        android:id="@+id/tv_heading_confirm_booking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_8"
        android:layout_marginStart="@dimen/margin_8"
        android:text="Confirm Bookings"
        style="@style/TextViewPageTitle"/>


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_confirm_booking"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_heading_confirm_booking"
        android:layout_marginTop="8dp"/>



</RelativeLayout>
